<?xml version="1.0" encoding="UTF-8"?>
<table name="User" display-name="user" old-name="Users">

	<column name="USER_ID">
		<data-type>INT</data-type>
		<auto-increment>AUTO_INCREMENT</auto-increment>
	</column>
	<column name="USER_NAME">
		<data-type>VARCHAR</data-type>
		<max-size>400</max-size>
		<nullable>NOT NULL</nullable>
	</column>
	<column name="PASSWORD">
		<data-type>VARCHAR</data-type>
		<max-size>300</max-size>
		<nullable>NOT NULL</nullable>
	</column>
	<column name="EMAIL">
		<data-type>VARCHAR</data-type>
		<max-size>300</max-size>
		<nullable>NOT NULL</nullable>
	</column>
	<column name="GENDER">
		<data-type>VARCHAR</data-type>
		<max-size>300</max-size>
		<nullable>NOT NULL</nullable>
	</column>
	<primary-key pk-name="pk_User">
		<key-column>USER_ID</key-column>
	</primary-key>
</table>